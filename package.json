{
  "name": "app",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./server.js",
    "dev": "run-p \"dev:*\"",
    "dev:live": "tsx --watch ./server-live.ts",
    "dev:react-router": "tsx ./server.ts",
    "build": "run-p \"build:*\"",
    "build:react-router": "react-router build",
    "build:server": "esbuild ./server.ts --outdir=./ --bundle --format=esm --platform=node --target=node24 --packages=external --external:./build/server/index.js",
    "test": "vitest",
    "typecheck": "react-router typegen && tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-router/express": "^7.13.0",
    "@react-router/node": "7.13.0",
    "classnames": "^2.5.1",
    "cross-env": "^10.1.0",
    "env-var": "^7.5.0",
    "express": "^5.2.1",
    "isbot": "^5.1.34",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "7.13.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@react-router/dev": "7.13.0",
    "@storybook/react-vite": "^10.2.1",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/express": "^5.0.6",
    "@types/node": "^25",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "clever-tools": "^4.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "happy-dom": "^20.3.9",
    "npm-run-all2": "^8.0.4",
    "storybook": "^10.1.11",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  }
}
